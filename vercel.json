{
  "functions": {
    "api/premium/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/premium/webhook.php"
    },
    {
      "src": "/check-status",
      "dest": "/api/premium/check-status.php"
    },
    {
      "src": "/premium/webhook",
      "dest": "/api/premium/webhook.php"
    },
    {
      "src": "/premium/check-status",
      "dest": "/api/premium/check-status.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}