{
  "functions": {
    "api/premium/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/premium/webhook.php"
    },
    {
      "source": "/check-status",
      "destination": "/api/premium/check-status.php"
    },
    {
      "source": "/premium/webhook",
      "destination": "/api/premium/webhook.php"
    },
    {
      "source": "/premium/check-status",
      "destination": "/api/premium/check-status.php"
    }
  ]
}
