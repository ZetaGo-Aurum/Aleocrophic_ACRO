{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.php"
    },
    {
      "src": "/check-status",
      "dest": "/api/check-status.php"
    },
    {
      "src": "/ACRO PREMIUM/webhook",
      "dest": "/api/webhook.php"
    },
    {
      "src": "/ACRO PREMIUM/check-status",
      "dest": "/api/check-status.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}